    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "graphql-import-node": "^0.0.4",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-tools": "^8.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.8",
    "ncp": "^2.0.0",
    "typescript": "^4.6.3"
